<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">




    <style name="AppTheme.Button" parent="@style/Widget.MaterialComponents.Button">
        <item name="colorButtonNormal">@color/secondaryColor</item>
        <item name="backgroundTint">@color/secondaryColor</item>
        <!--<item name="android:textColor">@color/White</item>-->
    </style>

    <style name="AppTheme.TextButton" parent="@style/Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">@color/secondaryColor</item>
        <!--<item name="android:textColor">@color/White</item>-->
    </style>

    <style name="Theme.CallStyle" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/white</item>
        <!--Secondary brand color.-->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="android:windowTranslucentStatus">true</item>
        <!--<item name="android:windowTranslucentNavigation">true</item>-->
    </style>



</resources>